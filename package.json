{
  "name": "react-combo-box",
  "version": "1.0.0",
  "description": "A React combo box component to handle primary and alternate field values.",
  "main": "dist/react-combo-box.js",
  "scripts": {
    "test": "jest",
    "cleantest": "jest  --updateSnapshot",
    "coverage": "jest --coverage",
    "prebuild": "rm -f dist/react-combo-box.js",
    "build": "babel src/react-combo-box.jsx --out-file dist/react-combo-box.js",
    "postbuild": "rm -f example/index.js; webpack example/index.jsx example/index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SokratisVidros/react-combo-box.git"
  },
  "keywords": [
    "react",
    "react-component",
    "combobox",
    "combo-box",
    "input",
    "listbox",
    "list-box",
    "primary",
    "alternate",
    "multivalues",
    "animated"
  ],
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-jest": "*",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "jest": "^19.0.2",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-test-renderer": "*",
    "regenerator-runtime": "*",
    "enzyme": "^2.7.1",
    "webpack": "2.3.1"
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x"
  },
  "author": "Sokratis Vidros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SokratisVidros/react-combo-box/issues"
  },
  "homepage": "https://github.com/SokratisVidros/react-combo-box",
  "dependencies": {
    "react-addons-test-utils": "^15.4.2",
    "react-flip-move": "^2.9.1",
    "underscore": "^1.8.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
